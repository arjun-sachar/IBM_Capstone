{
  "name": "Dashblocks",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .; stylelint client/**/*.less",
    "lint-fix": "eslint --fix --ext js .; eslint --fix --ext vue .; stylelint --fix client/**/*.less",
    "posttest": "npm run lint && nsp check",
    "dev": "npm run lb-start & npm run vue-serve",
    "prod": "npm run vue-build && npm start",
    "start": "node .",
    "lb-start": "nodemon",
    "vue-serve": "vue-cli-service serve client/src/main.js",
    "vue-build": "npx --max_old_space_size=8000 vue-cli-service build client/src/main.js --dest=client/dist",
    "docker-build-image": "docker build -t codeblue/dashblocks2 .",
    "docker-build": "npm run vue-build && npm run docker-build-image",
    "prettify": "prettier --write client/src/**/*.js && prettier --write client/src/**/*.less && prettier --write client/src/**/*.vue && prettier --write client/src/*.vue && prettier --write client/src/*.js"
  },
  "dependencies": {
    "@statnett/vue-plotly": "^0.3.2",
    "ant-design-vue": "^1.6.2",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "d3-request": "^1.0.6",
    "d3-scale": "^3.2.1",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "^1.4.1",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "event-stream": "^4.0.1",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.23.2",
    "kv-redis": "^0.1.3",
    "less": "^2.7.3",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "loopback": "^3.27.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.5.1",
    "loopback-connector-kv-redis": "^4.0.0",
    "loopback-connector-mysql": "^5.4.3",
    "loopback-connector-postgresql": "^5.0.1",
    "loopback-connector-redis": "^3.0.0",
    "moment": "^2.27.0",
    "nodemon": "^2.0.4",
    "plotly.js": "^1.54.5",
    "serve-favicon": "^2.5.0",
    "strong-error-handler": "^3.5.0",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-cookies": "^1.7.0",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-e2e-nightwatch": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-unit-mocha": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "Starter pack for user applications"
}
